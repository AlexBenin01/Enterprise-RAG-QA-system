[run]
source = src
omit = 
    */tests/*
    */__init__.py
    */presentation/gradio_ui.py
    */logging_config.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__:
    if TYPE_CHECKING:
    if settings.debug:
    @abstractmethod

skip_covered = False
precision = 2

[html]
directory = htmlcov
